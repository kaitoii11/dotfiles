ZDOTDIR=$HOME/.zsh
export LANG=ja_JP.UTF-8
export PYTHONPATH="/opt/local/Library/Frameworks/Python.framework/Versions/2.7/bin:/usr/local/lib/python2.7/site-packages:/usr/local/lib/python2.7/site-packages"
export JAVA_HOME="/Library/Java/JavaVirtualMachines/jdk1.8.0_31.jdk/Contents/Home"
export PATH="/opt/local/bin:/usr/local/bin:/opt/local/sbin:/usr/local/lib:/usr/local/mysql/bin:/opt/local/include:$PATH"
export MANPATH="/opt/local/share/man:/usr/local/man:/opt/local/man:$MANPATH"
export DYLD_FALLBACK_LIBRARY_PATH="$HOME/lib:/usr/local/lib:/lib:/usr/lib:/opt/local/lib"
export PKG_CONFIG_PATH="/usr/local/lib/pkgconfig"
typeset -U path PATH

